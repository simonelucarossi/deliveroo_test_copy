    "react": "18.0.0",
    "react-native": "0.69.4",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "tailwind-rn": "^4.2.0",
    "tailwindcss-react-native": "^1.7.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "concurrently": "^7.3.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8"
  },
  "private": true
